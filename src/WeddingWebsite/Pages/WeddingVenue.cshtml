@section Scripts {
    <script type="text/javascript">
        $(window).on('scroll', function () {
            function isScrollIntoView(elem, index) {
                var docViewTop = $(window).scrollTop();
                var docViewBottom = docViewTop + $(window).height();
                var elemTop = $(elem).offset().top;
                var elemBottom = elemTop + $(window).height() * .5;
                if (elemBottom <= docViewBottom && elemTop >= docViewTop) {
                    $(elem).addClass('active');
                }
                if (!(elemBottom <= docViewBottom)) {
                    $(elem).removeClass('active');
                }
                var MainTimelineContainer = $('#vertical-scrollable-timeline')[0];
                var MainTimelineContainerBottom = MainTimelineContainer.getBoundingClientRect().bottom - $(window).height() * .5;
                $(MainTimelineContainer).find('.inner').css('height', MainTimelineContainerBottom + 'px');
            }
            var timeline = $('#vertical-scrollable-timeline li');
            Array.from(timeline).forEach(isScrollIntoView);
        });
        // Some JavaScript code here to display map, etc.
        // Declare addMarker function
        function addMarker(latitude, longitude, Name, Adress, PostCode, City, Phone, map) {
            var latLng = new google.maps.LatLng(latitude, longitude);
            //var contentString = '<div class="card m-3" ><div class="card-header" ><h2>' + Name + '</h2></div> <div class="card-body" ><h5 class="card-title" > ' + Adress + '</h5> <h5 class="card-title">' + PostCode + ', ' + City + '</h5><p class="card-text">' + Phone + '</p></div> < /div>';
            var contentString = '<h5>' + Name + '</h5>';

            var infowindow = new google.maps.InfoWindow({
                content: contentString
            });
            const svgMarker = {
                //path: "M10.453 14.016l6.563-6.609-1.406-1.406-5.156 5.203-2.063-2.109-1.406 1.406zM12 2.016q2.906 0 4.945 2.039t2.039 4.945q0 1.453-0.727 3.328t-1.758 3.516-2.039 3.070-1.711 2.273l-0.75 0.797q-0.281-0.328-0.75-0.867t-1.688-2.156-2.133-3.141-1.664-3.445-0.75-3.375q0-2.906 2.039-4.945t4.945-2.039z",
                fillColor: "#F44336",
                fillOpacity: 0.6,
                strokeWeight: 0,
                rotation: 0,
                scale: 2,
                url: '/icons/lodging-2.png',
            };
            //var url = window.location.origin;
            // console.log(url);
            var marker = new google.maps.Marker({
                position: latLng,
                title: Name,
                map: map,
                draggable: false,
                icon: svgMarker
            });

            google.maps.event.addListener(marker, 'click', function () {
                infowindow.open(map, marker);
            });
        }

        function initMap() {
            const gaynes = { lat: 51.69589748304985, lng: 0.14796475731150335 };
            // The map, centered at Uluru
            const map = new google.maps.Map(document.getElementById("mapgaynespark"), {
                zoom: 10,
                center: gaynes,
            });
            const svgMarker = {
                //path: "M10.453 14.016l6.563-6.609-1.406-1.406-5.156 5.203-2.063-2.109-1.406 1.406zM12 2.016q2.906 0 4.945 2.039t2.039 4.945q0 1.453-0.727 3.328t-1.758 3.516-2.039 3.070-1.711 2.273l-0.75 0.797q-0.281-0.328-0.75-0.867t-1.688-2.156-2.133-3.141-1.664-3.445-0.75-3.375q0-2.906 2.039-4.945t4.945-2.039z",
                fillColor: "#F44336",
                fillOpacity: 0.6,
                strokeWeight: 0,
                rotation: 0,
                scale: 2,
                anchor: new google.maps.Point(15, 30),
                //url: "data:image/png;base64,",
            };
            var contentString = '<h3><bold>Gaynes Park</bold></h3>';

            var infowindow = new google.maps.InfoWindow({
                content: contentString
            });

            var mm = new google.maps.Marker({
                position: gaynes,
                icon: svgMarker,
                map: map,
                //fillColor: 'F44336',
                title: "Gaynes Park",
                //icon: "data:image/png;base64,"

            });
            google.maps.event.addListener(mm, 'click', function () {
                infowindow.open(map, mm);
            });
        @foreach (var item in Model.PlaceToSleeps)
        {
            @:addMarker(@item.lat, @item.lon, '@item.Name', '@item.Adress', '@item.PostCode', '@item.City', '@item.Phone', map);
        }
                                                                        }

        window.initMap = initMap;

    </script>
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyC730kPsZdAY0kzy8DkdcDHNmuo-GoSnvw&callback=initMap"
        defer></script>
}
